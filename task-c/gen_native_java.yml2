include yslt.yml2

textstylesheet {   
    template "entity" {
        ||
           public class «@name» {
               `` apply "nfunc|nfuncArr"
           }
        ||
    }


    template "nfunc" {
        ||
            public native «@rtype» «@fname»(«@ptype» «@pname»);
        ||
    }
    template "nfuncArr" {
        ||
            public native «@rtype» «@fname»(«@ptype»[] «@pname»);
        ||
    }
}
